{
  "name": "sentry",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "bootstrap": "npm i && lerna bootstrap",
    "clean": "lerna clean --yes",
    "publish": "lerna publish --skip-git",
    "test": "jest"
  },
  "jest": {
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/src/**/*.js*",
      "!**/test/*.js"
    ]
  },
  "devDependencies": {
    "babel-preset-stage-2": "^6.22.0",
    "eslint": "^3.18.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "lerna": "2.0.0-beta.38"
  }
}
